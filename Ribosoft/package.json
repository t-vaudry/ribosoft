{
  "name": "Ribosoft",
  "private": true,
  "version": "2.0.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2024,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es2024": true,
      "node": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.1",
    "@babel/plugin-transform-optional-chaining": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "@types/jquery": "^3.5.29",
    "@types/webpack-env": "^1.18.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/eslint-config-typescript": "^12.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.8",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.20.0",
    "core-js": "^3.37.0",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "event-source-polyfill": "^1.0.31",
    "expose-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "fornac": "^1.1.8",
    "glob": "^10.3.12",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.0",
    "jquery-ui-themes": "^1.12.0",
    "jquery-validation": "^1.20.0",
    "jquery-validation-unobtrusive": "^4.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "qrious": "^4.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "structured-filter": "^2.0.4",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.3",
    "url-loader": "^4.1.1",
    "vite": "^7.0.3",
    "vue": "^3.4.21",
    "vue-loader": "^17.4.2",
    "vue-select": "^4.0.0-beta.6",
    "vue-tsc": "^2.0.13",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.2.1",
    "webpack-hot-middleware": "^2.26.1"
  },
  "scripts": {
    "dev": "webpack --mode development --config webpack.config.js --watch --progress",
    "build": "webpack --mode production --config webpack.config.js --progress",
    "build:dev": "webpack --mode development --config webpack.config.js --progress",
    "build:vendor": "webpack --mode production --config webpack.config.vendor.js --progress",
    "build:vendor:dev": "webpack --mode development --config webpack.config.vendor.js --progress",
    "build:all": "npm run build:vendor && npm run build",
    "build:all:dev": "npm run build:vendor:dev && npm run build:dev",
    "clean": "rimraf wwwroot/dist .webpack-cache-vendor .webpack-cache-main",
    "clean:all": "rimraf node_modules package-lock.json && npm install",
    "lint": "eslint . --ext .js,.vue,.ts --fix",
    "lint:check": "eslint . --ext .js,.vue,.ts",
    "format": "prettier --write \"**/*.{js,vue,ts,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,vue,ts,css,scss,json,md}\"",
    "type-check": "vue-tsc --noEmit",
    "analyze": "webpack --mode production --config webpack.config.js --analyze",
    "serve": "webpack serve --mode development --config webpack.config.js",
    "test": "echo \"No tests specified\" && exit 0",
    "audit:fix": "npm audit fix"
  }
}
